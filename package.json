{
  "name": "blockhead",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run clean:houdini; vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate": "npx npm-run-all generate:*",
    "generate:swagger-typescript-api:zapper": "swagger-typescript-api --path https://api.zapper.fi/api/json --output ./src/data/zapper/api/ --clean-output true --responses true --union-enums true --add-readonly true --route-types false --enum-names-as-values true --extract-request-params true --extract-request-body true --modular true --unwrap-response-data true --single-http-client true --default-response unknown",
    "generate:swagger-typescript-api:moralis": "swagger-typescript-api --path https://deep-index.moralis.io/api-docs/v2/swagger.json --output ./src/data/moralis/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:swagger-typescript-api:nftport": "swagger-typescript-api --path https://api.nftport.xyz/openapi.json --output ./src/data/analytics/nftport/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:houdini": "houdini generate --pull-schema",
    "clean:houdini": "rm -rf './$houdini'; rm ./src/**/*.graphql"
  },
  "type": "module",
  "dependencies": {
    "@3id/connect": "^0.1.6",
    "@apollo/client": "^3.2.0",
    "@ceramicnetwork/3id-did-resolver": "^2.1.8",
    "@ceramicnetwork/common": "^2.7.0",
    "@ceramicnetwork/http-client": "^2.4.0",
    "@ceramicnetwork/stream-caip10-link": "^2.2.4",
    "@ceramicnetwork/stream-tile": "^2.4.4",
    "@ceramicnetwork/streamid": "^2.3.4",
    "@coinbase/wallet-sdk": "^3.2.0",
    "@compound-finance/compound-js": "^0.2.13",
    "@connext/vector-browser-node": "^0.2.5-beta.18",
    "@connext/vector-modal": "^1.0.6-beta.6",
    "@connext/vector-sdk": "^0.0.3-beta.6",
    "@connext/vector-types": "^0.2.5-beta.18",
    "@connext/vector-utils": "^0.2.5-beta.18",
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/abstract-signer": "^5.6.2",
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/constants": "^5.6.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.4",
    "@ethersproject/units": "^5.6.0",
    "@ethersproject/wallet": "^5.6.0",
    "@fontsource/fira-code": "^4.5.1",
    "@fontsource/ubuntu": "^4.5.0",
    "@portis/web3": "^4.0.5",
    "@self.id/web": "^0.4.3",
    "@sveltestack/svelte-query": "^1.6.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "@web3api/client-js": "^0.0.1-prealpha.44",
    "bip44-constants": "^65.0.0",
    "buffer": "^6.0.3",
    "dids": "^2.4.0",
    "dotenv": "^10.0.0",
    "echarts": "^5.2.1",
    "ethers": "^5.6.8",
    "ethers-to-web3": "^0.1.6",
    "etherspot": "^1.33.2",
    "events": "^3.3.0",
    "graphql": "^15.5.1",
    "key-did-resolver": "^1.2.1",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2",
    "subscriptions-transport-ws": "^0.9.19",
    "svelte-apollo": "^0.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-auto": "1.0.0-next.72",
    "@sveltejs/kit": "1.0.0-next.503",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "broadcast-channel": "^4.13.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "houdini": "^0.16.5",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "swagger-typescript-api": "^10.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}