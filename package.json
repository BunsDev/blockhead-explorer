    "@portis/web3": "^4.0.7",
    "@sveltestack/svelte-query": "^1.6.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "@web3api/client-js": "^0.0.1-prealpha.83",
    "bip44-constants": "^129.0.0",
    "buffer": "^6.0.3",
    "dids": "^3.2.0",
    "dotenv": "^16.0.1",
    "echarts": "^5.3.2",
    "ethers": "^5.6.8",
    "ethers-to-web3": "^0.1.6",
    "etherspot": "^1.33.2",
