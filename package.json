    "@ceramicnetwork/http-client": "^2.0.4",
    "@ceramicnetwork/stream-caip10-link": "^2.0.4",
    "@ceramicnetwork/stream-tile": "^2.1.3",
    "@ceramicnetwork/streamid": "^2.0.4",
    "@ceramicstudio/idx": "^0.12.1",
    "@ceramicstudio/idx-constants": "^1.0.0",
    "@coinbase/wallet-sdk": "^3.2.0",
    "@compound-finance/compound-js": "^0.4.4",
    "@connext/vector-browser-node": "^0.2.5-beta.21",
    "@connext/vector-modal": "^1.0.6-beta.7",
    "@connext/vector-sdk": "^0.0.3-beta.7",
    "@connext/vector-types": "^0.2.5-beta.21",
    "@connext/vector-utils": "^0.2.5-beta.21",
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/abstract-signer": "^5.6.2",
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/constants": "^5.6.1",
