{
  "name": "blockhead",
  "version": "0.0.1",
  "homepage": "https://blockhead.info",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:static": "SVELTE_BUILD_STATIC=1 vite build --mode static",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate": "npx npm-run-all generate:*",
    "generate:swagger-typescript-api:zapper": "swagger-typescript-api --path https://api.zapper.fi/api/json --output ./src/api/zapper/api/ --clean-output true --responses true --union-enums true --add-readonly true --route-types false --enum-names-as-values true --extract-request-params true --extract-request-body true --modular true --unwrap-response-data true --single-http-client true --default-response unknown",
    "generate:swagger-typescript-api:moralis": "swagger-typescript-api --path https://deep-index.moralis.io/api-docs/v2/swagger.json --output ./src/api/moralis/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:swagger-typescript-api:nftport": "swagger-typescript-api --path https://api.nftport.xyz/openapi.json --output ./src/api/nftport/api/ --clean-output true --union-enums false --route-types true --enum-names-as-values true --extract-request-params true --extract-request-body true --single-http-client true --default-response {} --unwrap-response-data true",
    "generate:houdini": "houdini generate --pull-schema",
    "clean:houdini": "rm -rf './$houdini'; rm ./src/**/*.graphql"
  },
  "type": "module",
  "packageManager": "pnpm@8.6.3",
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@coinbase/wallet-sdk": "^3.7.1",
    "@fontsource/fira-code": "^5.0.3",
    "@fontsource/ubuntu": "^5.0.3",
    "@helia/unixfs": "^1.3.0",
    "@liquality/wallet-sdk": "^2.4.1",
    "@pushprotocol/restapi": "^1.3.8",
    "@pushprotocol/socket": "^0.5.1",
    "@rize-labs/banana-wallet-sdk": "^0.1.24",
    "@sveltestack/svelte-query": "^1.6.0",
    "@urql/svelte": "^4.0.2",
    "@walletconnect/core": "^2.8.1",
    "@walletconnect/ethereum-provider": "^2.8.1",
    "@walletconnect/jsonrpc-ws-connection": "^1.0.11",
    "@walletconnect/relay-api": "^1.0.9",
    "@walletconnect/sign-client": "^2.8.1",
    "@walletconnect/types": "^2.8.1",
    "@walletconnect/web3-provider": "^1.8.0",
    "@walletconnect/web3wallet": "^1.8.1",
    "@web3modal/standalone": "^2.4.3",
    "array-from-async": "^3.0.0",
    "array.prototype.group": "^1.1.2",
    "array.prototype.grouptomap": "^1.1.2",
    "bip44-constants": "^225.0.0",
    "blockstore-core": "^4.2.0",
    "blockstore-idb": "^1.1.1",
    "buffer": "^6.0.3",
    "datastore-core": "^9.2.0",
    "datastore-idb": "^2.1.2",
    "detect-content-type": "^1.2.0",
    "echarts": "^5.4.2",
    "ethers": "^6.6.0",
    "etherspot": "^1.43.7",
    "file-type": "^18.5.0",
    "graphql": "^16.7.1",
    "helia": "^1.3.3",
    "key-did-resolver": "^3.0.0",
    "multiformats": "^12.0.1",
    "process": "^0.11.10",
    "runes2": "^1.1.2",
    "svelte-apollo": "^0.5.0",
    "svelte-meta-tags": "^2.8.0",
    "svelvet": "^6.0.12",
    "uint8arrays": "^4.0.4",
    "webmax": "^0.2.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@playwright/test": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "houdini": "^1.2.5",
    "houdini-svelte": "^1.2.5",
    "pnpm": "^8.6.3",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "svelte-preprocess": "^5.0.4",
    "swagger-typescript-api": "^12.0.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "pnpm": {
    "patchedDependencies": {
      "@pushprotocol/restapi@1.3.1": "patches/@pushprotocol__restapi@1.3.1.patch"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}